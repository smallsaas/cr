<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.cr.services.persistence.dao.ModuleMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.jfeat.am.module.cr.services.persistence.model.Module">
		<id column="id" property="id" />
		<result column="project_id" property="projectId" />
		<result column="note" property="note" />
		<result column="module_name" property="moduleName" />
		<result column="create_time" property="createTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, project_id AS projectId, note, module_name AS moduleName, create_time AS createTime
    </sql>

</mapper>
